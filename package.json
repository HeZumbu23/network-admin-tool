{
  "name": "network-admin-tool",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/frontend"
  ],
  "scripts": {
    "dev": "dotnet run --project packages/backend & npm run dev --workspace=packages/frontend",
    "dev:backend": "dotnet run --project packages/backend",
    "dev:backend:watch": "dotnet watch --project packages/backend run",
    "dev:frontend": "npm run dev --workspace=packages/frontend",
    "build:frontend": "npm run build --workspace=packages/frontend",
    "build:backend": "dotnet publish packages/backend -c Release -o packages/backend/publish"
  }
}
